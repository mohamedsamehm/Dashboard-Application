                        .checkbox
                          input#reddit(type='checkbox', value='Reddit', name='redditd')
                          label.dropdown-option(for='reddit') Reddit
                        .checkbox
                          input#twitter(type='checkbox', value='Twitter', name='twitter')
                          label.dropdown-option(for='twitter') Twitter
                        .checkbox
                          input#blogs(type='checkbox', value='Blogs', name='Blogs')
                          label.dropdown-option(for='blogs') Blogs
                        .checkbox
                          input#thumblr(type='checkbox', value='Thumblr', name='Thumblr')
