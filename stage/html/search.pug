          .magic-container.search-container
            .tags
              .row.no-gutters#cycleitems
                .col.tags-block.scrollbar
                  h2
                    .text Suggestions
                  .thetags
                    - for (var i = 0; i < 50; ++i)
                      .tag + healthy
                .col.tags-block.scrollbar
                  h2 
                      .text Related
                  .thetags
                      - for (var i = 0; i < 50; ++i)
                        .tag + healthy
                .col.tags-block.scrollbar
                  h2 
                    .text Translations
                  .thetags
                    - for (var i = 0; i < 50; ++i)
                      .tag + healthy
            .bar.d-block.d-lg-none
